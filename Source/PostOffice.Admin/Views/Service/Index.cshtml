@model IEnumerable<PostOffice.API.DTOs.ParcelService.ParcelServiceBaseDTO>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<!-- Content Wrapper. Contains page content -->
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1>Employee List</h1>
                </div>
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="#">Home</a></li>
                        <li class="breadcrumb-item active">Parcel Order List</li>
                    </ol>
                </div>
            </div>
        </div><!-- /.container-fluid -->
    </section>

    <!-- Main content -->
    <section class="content">
        <div class="container-fluid">
            <div class="row">
                <div class="col-12">
                    <div class="card">
                        <div class="card-header">
                            <h3 class="card-title">  <a class="btn btn-success" asp-action="Index" asp-controller="Service">Parcel Service</a></h3>

                        
                        </div>
                        <!-- /.card-header -->
                            <div class="card-body table-responsive p-0">
                <table class="table table-hover text-nowrap">
                    
                    <thead>
                        <tr>
                            <th>Name</th>
                            <th>Description</th>
                            <th>Status</th>
                            <th>Delivery Time</th>
                            <th>Action</th>
                        </tr>
                    </thead>
                    @foreach (var item in Model){
                    <tbody>
                            <tr>
                                <td>@item.name</td>
                                <td>@item.description</td>
                                <td>@item.status</td>
                                <td>@item.delivery_time</td>
                                <td>@Html.ActionLink("Edit", "Edit", new {id = item.service_id})</td>
                            </tr>
                       
                    </tbody>
                    }
                </table>
            </div>
                    </div>
                    <!-- /.card -->
                </div>
            </div>
        </div><!-- /.container-fluid -->
    </section>
    <!-- /.content -->
<!-- /.content-wrapper -->
