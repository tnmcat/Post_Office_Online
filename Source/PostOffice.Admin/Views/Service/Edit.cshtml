
@model PostOffice.API.DTOs.ParcelService.ParcelServiceUpdateDTO
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1>Employee List</h1>
                </div>
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="#">Home</a></li>
                        <li class="breadcrumb-item active">Parcel Service</li>
                    </ol>
                </div>
            </div>
        </div><!-- /.container-fluid -->
    </section>

    <!-- Main content -->
    <section class="content">
        <div class="container-fluid">
            <div class="row">
                <div class="col-12">
                    <div class="card">
                        <div class="card-header">
                            <h3 class="card-title">  <a class="btn btn-success" asp-action="Edit" asp-controller="Service">Parcel Service</a></h3>


                        </div>
                        <!-- /.card-header -->
                        <div class="card-body card-block">
                            <form asp-action="Edit" asp-controller="Service" method="post" class="form-horizontal">
                                <div class="row form-group">
                                    <div class="col col-sm-5"><label for="input-normal" class=" form-control-label">Delivery Time</label></div>
                                    <div class="col col-sm-6"><input type="number" asp-for="delivery_time" placeholder="Normal" class="form-control"></div>
                                </div>
                                <div class="card-footer">
                                    <input type="submit" values="Edit" class="btn btn-primary btn-sm">
                                    </input>
                                    <input type="reset" class="btn btn-danger btn-sm">
                                    <i class="fa fa-ban"></i>
                                    </input>
                                </div>
                            </form>
                        </div>
                    </div>
                    <!-- /.card -->
                </div>
            </div>
        </div><!-- /.container-fluid -->
    </section>
    <!-- /.content -->
</div>
<!-- /.content-wrapper -->
