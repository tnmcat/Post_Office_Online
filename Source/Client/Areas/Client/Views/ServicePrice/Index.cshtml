@model IEnumerable<PostOffice.API.DTOs.ParcelServicePrice.ServicePriceExpress>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Client/Views/Shared/_LayoutClient.cshtml";
}

<div class="col-md-12">
    <div class="card">
        <div class="card-header">
            <strong class="card-title">Express Service Price</strong>
        </div>
        <p>
            TExpress Order Service pricing is based on the weight and dimensions of the shipment, as well as the desired delivery service level. 
            You can get a shipping estimate on our website or at your local Post Office.
        </p>
        <div class="card-body">
            <table id="bootstrap-data-table-export" class="table table-striped table-bordered">
                <thead>
                    <tr>
                        <th>Weight Scope</th>
                        <th>Zone</th>
                        <th style="width: 40px">Price</th>
                    </tr>
                </thead>
                @foreach (var item in Model)
                {

                    <tbody>
                        <tr>
                            <td>@item.Description</td>
                            <td>@item.ZoneDescription</td>
                            <td>@item.ServicePrice</td>

                        </tr>

                    </tbody>
                }
            </table>
        </div>
    </div>
</div>

